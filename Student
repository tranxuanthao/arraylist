
public class Student {
	private String name;
	private int id;
	private int mathScope;
	private int engScope;
	public int getEngScope() {
		return engScope;
	}
	public void setEngScope(int engScope) {
		this.engScope = engScope;
	}
	
	public Student(String name, int id, int mathScope, int engScope) {
		super();
		this.name = name;
		this.id = id;
		this.mathScope = mathScope;
		this.engScope = engScope;
	}
	public int getMathScope() {
		return mathScope;
	}
	public void setMathScope(int mathScope) {
		this.mathScope = mathScope;
	}
	public String getName() {
		return name;
	}
	@Override
	public String toString() {
		return this.name+" "+this.id+" "+this.mathScope+" "+this.engScope;
	}
}
